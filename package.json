{
  "name": "partnership-mapping-tool",
  "version": "0.0.1",
  "description": "Interactive format to plot and analyze collaborative networks",
  "main": "dist",
  "engines": {
    "node": "8.9.0"
  },
  "scripts": {
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "client": "cd ../client && yarn start",
    "server": "nodemon --inspect-brk -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "dev": "concurrently --kill-others-on-fail \"yarn server\""
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Saeed Alalwan",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^6.0.0",
    "express": "^4.13.3",
    "express-session": "^1.15.6",
    "mongoose": "^5.1.5",
    "morgan": "^1.8.0",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "resource-router-middleware": "github:alwasa0b/resource-router-middleware"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
